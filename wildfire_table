CREATE TABLE IF NOT EXISTS wildfiretable (
    OBJECTID BIGINT,
    FIRE_YEAR BIGINT,
    FIRE_NUM TEXT,
    VERSN_NUM BIGINT,
    FIRE_SZ_HA DOUBLE,
    SOURCE TEXT,
    TRACK_DATE DATE,
    LOAD_DATE DATE,
    FIRE_STAT TEXT,
    FIRE_LINK TEXT,
    FEATURE_CD TEXT,
    ID UUID,
    PRIMARY KEY (FIRE_NUM, ID)
);